<?xml version="1.0" encoding="UTF-8"?>
<cronentries>

    <cron>
        <url>/keepWorking.faces</url>
        <description> Keep the app not to disabled </description>
        <schedule>every 3 minutes</schedule>
    </cron>

    <cron>
        <url>/task/</url>
        <description> Execute Tasks </description>
        <schedule>every 30 minutes</schedule>
    </cron>

    <cron>
        <url>/ah/SessionCleanup?clear</url>
        <description>Clean up sessions</description>
        <schedule>every 2 hours</schedule>
    </cron>

